@import '../../template/wxss/common.wxss';
@import '../../template/wxss/photo_task.wxss';

.card {
  background-image: url('data:image/png;base64,');
}

.sfz {
  display: block;
  text-align: center;
  padding: 20rpx 40rpx;
}

.sfzname {
  padding: 10rpx 0;
  text-align: center;
  color: blue;
  font-size: 30rpx;
  margin: 20rpx 0;
}

.uploadphoto {
  position: relative;
  height: 12em;
  overflow: hidden;
}

.sfz image {
  position: absolute;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  -webkit-object-fit: contain;
  -moz-object-fit: contain;
  -ms-object-fit: contain;
  -o-object-fit: contain;
  object-fit: contain;
  width: 100%;
  height: 100%;
  display: inline-block;
}

.button-sp-area button {
  margin-top: 20rpx;
  margin-bottom: 50rpx;
}

.container {
  padding: 20rpx 40rpx;
}
