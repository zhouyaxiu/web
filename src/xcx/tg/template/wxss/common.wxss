.rem1 {
  padding: 20rpx 0;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  border-bottom: 10px solid #f0f0f0;
  margin-bottom: 20px;
}

.weui-cell {
  font-size: 30rpx;
}

.weui-cell:before {
  bottom: 0;
  top: 100%;
  right: 30rpx;
}

.weui-cell_ft {
  width: 40%;
  text-align: right;
  float: right;
}

.btn-area {
  width: 80%;
  margin: 80rpx auto 0;
}

.txt-area {
  margin-top: 30rpx;
  display: flex;
  justify-content: space-between;
  font-size: 30rpx;
  width: 80%;
  margin: 30rpx auto;
  text-align: center;
}

.txt-area text {
  position: absolute;
  bottom: 40rpx;
  left: 0;
  right: 0;
  margin: auto;
}

.logo {
  text-align: center;
  padding: 30rpx;
  background: #15b3e4;
  margin-bottom: 20rpx;
}

.weui-cell_ft button {
  color: #fff;
  font-size: 26rpx;
  float: right;
}

.logo image {
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  animation: bd-rise 2s ease-in-out;
  opacity: 0.95;
  background: rgba(0, 194, 255, 0.8);
}

.default {
  display: block;
  width: 80%;
  text-align: center;
  font-size: 28rpx;
  color: #999;
}

.weui-input .active {
  border: 2rpx solid #15b3e4;
}

.weui-label {
  width: 40rpx;
  height: 40rpx;
  background-size: 40rpx 40rpx;
  margin-right: 40rpx;
}

.login {
  background-image: url('data:image/png;base64,');
}

.password {
  background-image: url('data:image/png;base64,');
}

.mobile {
  background-image: url('data:image/png;base64,');
}

.yzm {
  background-image: url('data:image/png;base64,');
  width: 44rpx;
  height: 44rpx;
  background-size: 44rpx 44rpx;
}

.rem {
  font-size: 30rpx;
  padding: 30rpx 0 0 40rpx;
  display: block;
  text-align: left;
  float: left;
}

.weui-btn_default {
  color: #000 !important;
  background: #f8f8f8 !important;
  margin-left: 20rpx;
}

.button-sp-area button {
  margin-top: 20px;
  margin-bottom: 50px;
}

.country {
  background: rgba(0, 0, 0, .6);
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  margin: auto;
  z-index: 999;
}

.countrycode {
  position: absolute;
  top: 10%;
  left: 40rpx;
  bottom: 10%;
  right: 40rpx;
  background: #fff;
  overflow-y: auto;
  z-index: 1000;
}

.countrycode view {
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 30rpx 40rpx;
  font-size: 34rpx;
  border-bottom: 2rpx solid #f0f0f0;
}

.countrycode text {
  flex: 1
}

.countrycode text:last-child {
  text-align: right;
  max-width: 160rpx;
}